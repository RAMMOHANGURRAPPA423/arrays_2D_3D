#include<stdio.h>
int main(){
    int i,j,k;
    int arr[2][2][2]={10,20,30,40,50,60,70,80};
    printf("%d\n",arr[0][0][0]);
    printf("%d\n",arr[1][1][0]);
     printf("%p",arr[0][0][0]);
     for(i=0;i<2;i++){
         for(j=0;j<2;j++){
             for(k=0;k<2;k++){
                 printf("&arr[%d][%d][%d]=%p,value is =%d\n",i,j,k,&arr[i][j][k],arr[i][j][k]);
             }
         }
     }
}
