#include <stdio.h>

int main()
{
  int arr[2][3],i,j;
  printf("enter the elements of array:");
  for(i=0;i<2;i++){
      for(j=0;j<3;j++){
          scanf("%d",&arr[i][j]);
      }
  }

printf("The matrix is:");
for(i=0;i<2;i++){
    for(j=0;j<3;j++){
        printf("%d",arr[i][j]);
    }
    printf("\n");
}
}
